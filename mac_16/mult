#! /usr/bin/vvp
:ivl_version "10.1 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 9;
:vpi_module "system";
:vpi_module "vhdl_sys";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x55d8b9df4480 .scope module, "tb_mult_16_V1" "tb_mult_16_V1" 2 3;
 .timescale -9 -9;
v0x55d8b9e14b80_0 .var "a", 15 0;
v0x55d8b9e14c70_0 .var "b", 15 0;
v0x55d8b9e14d40_0 .var "clk", 0 0;
v0x55d8b9e14e10_0 .net "op", 31 0, v0x55d8b9e14a20_0;  1 drivers
S_0x55d8b9df4600 .scope module, "m1" "mult_16_V1" 2 7, 3 1 0, S_0x55d8b9df4480;
 .timescale 0 0;
    .port_info 0 /INPUT 16 "a"
    .port_info 1 /INPUT 16 "b"
    .port_info 2 /OUTPUT 32 "op"
v0x55d8b9df4820_0 .net "a", 15 0, v0x55d8b9e14b80_0;  1 drivers
v0x55d8b9e14880_0 .net "b", 15 0, v0x55d8b9e14c70_0;  1 drivers
v0x55d8b9e14960_0 .var/i "i", 31 0;
v0x55d8b9e14a20_0 .var "op", 31 0;
E_0x55d8b9deeae0 .event edge, v0x55d8b9e14880_0, v0x55d8b9df4820_0;
    .scope S_0x55d8b9df4600;
T_0 ;
    %wait E_0x55d8b9deeae0;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x55d8b9e14a20_0, 0, 32;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x55d8b9e14960_0, 0, 32;
T_0.0 ;
    %load/vec4 v0x55d8b9e14960_0;
    %cmpi/s 16, 0, 32;
    %jmp/0xz T_0.1, 5;
    %load/vec4 v0x55d8b9e14880_0;
    %load/vec4 v0x55d8b9e14960_0;
    %part/s 1;
    %cmpi/e 1, 0, 1;
    %jmp/0xz  T_0.2, 4;
    %load/vec4 v0x55d8b9e14a20_0;
    %load/vec4 v0x55d8b9df4820_0;
    %pad/u 32;
    %load/vec4 v0x55d8b9e14960_0;
    %ix/vec4 4;
    %shiftl 4;
    %add;
    %store/vec4 v0x55d8b9e14a20_0, 0, 32;
T_0.2 ;
    %load/vec4 v0x55d8b9e14960_0;
    %addi 1, 0, 32;
    %store/vec4 v0x55d8b9e14960_0, 0, 32;
    %jmp T_0.0;
T_0.1 ;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_0x55d8b9df4480;
T_1 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55d8b9e14d40_0, 0, 1;
    %end;
    .thread T_1;
    .scope S_0x55d8b9df4480;
T_2 ;
    %delay 5, 0;
    %load/vec4 v0x55d8b9e14d40_0;
    %inv;
    %store/vec4 v0x55d8b9e14d40_0, 0, 1;
    %jmp T_2;
    .thread T_2;
    .scope S_0x55d8b9df4480;
T_3 ;
    %vpi_call 2 12 "$monitor", "a = %d, b = %d, sum = %d", v0x55d8b9e14b80_0, v0x55d8b9e14c70_0, v0x55d8b9e14e10_0 {0 0 0};
    %vpi_call 2 13 "$dumpfile", "mult_test.vcd" {0 0 0};
    %vpi_call 2 14 "$dumpvars", 32'sb00000000000000000000000000000000 {0 0 0};
    %delay 10, 0;
    %pushi/vec4 10, 0, 16;
    %store/vec4 v0x55d8b9e14b80_0, 0, 16;
    %pushi/vec4 10, 0, 16;
    %store/vec4 v0x55d8b9e14c70_0, 0, 16;
    %delay 10, 0;
    %pushi/vec4 15, 0, 16;
    %store/vec4 v0x55d8b9e14b80_0, 0, 16;
    %pushi/vec4 6, 0, 16;
    %store/vec4 v0x55d8b9e14c70_0, 0, 16;
    %delay 10, 0;
    %vpi_call 2 22 "$finish" {0 0 0};
    %end;
    .thread T_3;
# The file index is used to find the file name in the following table.
